{
  "name": "TuneCrumble",
  "description": "Smart, controllable replication for your iTunes library.",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": [
        "background/q.js",
        "background/util.js",
        "background/cmd.js",
        "background/view_updater_registry.js",
        "background/storage.js",
        "background/persistence.js",
        "background/itunes_library_info.js",
        "background/background.js"
      ],
      "persistent": false
    }
  },
  "permissions": [
    { "fileSystem": ["write", "retainEntries", "directory"] },
    "storage",
    "contextMenus"
  ],
  "icons": { "16": "skin/tunecrumble-16.png", "128": "skin/tunecrumble-128.png" }
}