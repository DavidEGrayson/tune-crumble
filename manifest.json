{
  "name": "TuneCrumble",
  "description": "Smart, controllable replication for your iTunes library.",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["util.js", "persistence.js", "itunes_library_info.js", "background.js"],
      "persistent": false
    }
  },
  "permissions": [
    { "fileSystem": ["write", "retainEntries", "directory"] },
    "storage",
    "contextMenus"
  ],
  "icons": { "16": "tunecrumble-16.png", "128": "tunecrumble-128.png" }
}